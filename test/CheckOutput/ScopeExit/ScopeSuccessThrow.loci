// Test output.
import void testPrint(const ubyte * str, ...) noexcept;

exception TestError();

// Entry point.
export int testEntryPoint(unused int argc, unused ubyte ** argv) {
	try {
		scope (exit) {
			testPrint(C"testEntryPoint: Exiting try scope.");
		}
		
		scope (failure) {
			testPrint(C"testEntryPoint: Failure case.");
		}
		
		scope (success) {
			testPrint(C"testEntryPoint: Success case; throwing exception.");
			throw TestError();
		}
		
		testPrint(C"testEntryPoint: Returning 1.");
		return 1;
	} catch (TestError e) {
		testPrint(C"testEntryPoint: Caught exception; returning 0.");
		return 0;
	}
}

