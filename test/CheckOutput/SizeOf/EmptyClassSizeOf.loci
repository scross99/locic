// Test output.
import void testPrint(const ubyte * str, ...) noexcept;

class EmptyClass() { }

// Entry point.
export int testEntryPoint(unused int argc, unused ubyte ** argv) {
	testPrint(C"sizeof(EmptyClass) = %llu", sizeof(EmptyClass).cast<ulonglong_t>());
	testPrint(C"EmptyClass.__sizeof() = %llu", EmptyClass.__sizeof().cast<ulonglong_t>());
	return 0;
}

