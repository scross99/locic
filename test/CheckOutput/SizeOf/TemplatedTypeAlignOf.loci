// Test output.
import void testPrint(const ubyte * str, ...);

template <typename T: sized_type_t>
size_t getTypeAlign() {
	return alignof(T);
}

// Entry point.
export int testEntryPoint(unused int argc, unused ubyte ** argv) {
	testPrint(C"getTypeAlign<int>() = %llu", getTypeAlign<int>().cast<ulonglong_t>());
	return 0;
}

