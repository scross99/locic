__primitive typename_t {
	static size_t __alignmask() noexcept;
	static size_t __sizeof() noexcept;
	
	void __destroy() noexcept;
	
	typename_t implicit_copy() const noexcept;
	typename_t copy() const noexcept;
	
	typename_t __move() noexcept;
}

interface typename_hasher_t {
	void hash_typename(typename_t value) noexcept;
}

void typename_t::hash(typename_hasher_t& hasher) const noexcept {
	hasher.hash_typename(self);
}
