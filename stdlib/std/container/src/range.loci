export std.container 0.1.0 {

	namespace std {
		
		template <typename T>
		interface forward_range {
			bool empty() const;
			
			void skip_front();
			
			T front() const;
		}
		
		template <typename T>
		interface backward_range {
			bool empty() const;
			
			void skip_back();
			
			T back() const;
		}
		
		template <typename T>
		interface bidirectional_range {
			bool empty() const;
			
			void skip_front();
			
			T front() const;
			
			void skip_back();
			
			T back() const;
		}
		
	}
	
}
		
		
